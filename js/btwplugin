window.onload = function() {
    themeDefaultContent = $(
      '#app > .theme-container>.page > .theme-default-content'
    );
   
    themeDefaultContent.attr('id', 'container');
    btw = new BTWPlugin(); // 注意btw需要是个全局变量,把const去掉
    btw.init({
        id: 'container',
        blogId: '33542-1731577134824-389',
        name: '程序员清宇',
        qrcode: 'https://img-qywx.oss-cn-beijing.aliyuncs.com/qrcode.jpg',
        keyword: 'qy',
    });
  };